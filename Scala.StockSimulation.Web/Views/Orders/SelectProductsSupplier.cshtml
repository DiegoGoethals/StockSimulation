@model SelectProductsSupplierViewModel
<link rel="stylesheet" href="/css/Shared.css" />

<partial name="_LogoutButton" />
<div class="content">
    <h1 class="display-4">Kies een leverancier</h1>
    <p>Maak een selectie:</p>
</div>
<div class="form-group">
    <div class="form-group custom-select-group">
        <form asp-controller="Orders" asp-action="SelectProductsSupplier" method="post">
        <select class="form-control custom-select" asp-for="SupplierId" asp-items="@Model.Supplier">
        </select>
            <button type="submit" class="btn btn-secondary mt-5" >Bevestig</button>
        </form>
    </div>
</div>




